

@media (min-width: 1337px) {
    .tableCustomHeight {
        height: 640px;
    }
    
    .tableCustomHeight .ant-table{
        height: 575px;
        padding-bottom: 50px;
    }
    
    .tableCustomHeight .ant-table.ant-table-empty .ant-table-tbody {
        height: 500px;
    }
}

@media (min-width: 1200px) and (max-width: 1336.99998px) {
    .tableCustomHeight {
        height: 610px;
    }
    
    .tableCustomHeight .ant-table{
        height: 545px;
        padding-bottom: 50px;
    }
    
    .tableCustomHeight .ant-table.ant-table-empty .ant-table-tbody {
        height: 470px;
    }
}

@media (min-width: 1200px) {
    .border-right {
        border-right: 1px solid #d9d9d9;
    }
}